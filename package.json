{
  "name": "ts-nameof",
  "version": "2.0.0",
  "description": "nameof in TypeScript",
  "main": "dist/main.js",
  "typings": "ts-nameof.d.ts",
  "scripts": {
    "test": "yarn run --silent copy-test-files && nyc --reporter=lcov mocha --opts mocha.opts",
    "build": "rimraf dist && tsc",
    "copy-test-files": "rimraf temp && copyfiles -u 2 \"./src/tests/testFiles/**/*{.js,.txt}\" \"./temp\"",
    "lint": "tslint src/**/*.ts",
    "dopublish": "yarn run lint && npm run test && yarn run build && npm publish --otp",
    "maintain-barrels": "barrel-maintainer src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/ts-nameof.git"
  },
  "keywords": [
    "typescript",
    "transformer",
    "custom-transformer"
  ],
  "author": "David Sherret",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsherret/ts-nameof/issues"
  },
  "homepage": "https://github.com/dsherret/ts-nameof#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "!src/tests/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "typescript": {
    "definition": "ts-nameof.d.ts"
  },
  "dependencies": {
    "glob": "^7.1.3"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.4",
    "barrel-maintainer": "^1.5.0",
    "conditional-type-checks": "^0.5.0",
    "copyfiles": "^2.0.0",
    "coveralls": "^3.0.2",
    "gulp": "^3.9.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
