{
  "name": "babel-plugin-ts-nameof",
  "version": "0.3.0",
  "description": "nameof in TypeScript for babel.",
  "main": "dist/index.js",
  "scripts": {
    "install-dependencies": "ts-node ../../scripts/runInSeries install-build transforms-common",
    "create-links": "symlink-dir ../../shared/transforms-babel/dist src/external/transforms-babel",
    "prepare": "yarn install-dependencies && yarn create-links",
    "build": "rimraf dist && tsc --build && cpx \"./src/external/**/*.js\" \"./dist/external\" -L",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json src/**/*.ts",
    "dopublish": "yarn install && yarn build && echo \"Run: npm publish --otp\""
  },
  "keywords": [
    "nameof",
    "typescript",
    "transforms",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/ts-nameof.git",
    "directory": "packages/babel-plugin-ts-nameof"
  },
  "author": "David Sherret",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/babel__core": "^7.1.2",
    "@types/babel__generator": "^7.0.2",
    "@types/babel__template": "^7.0.2",
    "@types/babel__traverse": "^7.0.7",
    "@types/node": "^12.6.5",
    "cpx": "^1.5.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "symlink-dir": "^3.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
